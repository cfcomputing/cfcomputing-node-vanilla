{
	"env": {
		"browser": false,
		"node": true,
		"jest": true
	},
	"extends": ["eslint:recommended", "plugin:node/recommended"],
	"parser": "babel-eslint",
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": 9
	},
	"rules": {
		"indent": ["error", "tab"],
		"node/no-unsupported-features/es-syntax": "off",
		"no-console": "off"
	}
}
